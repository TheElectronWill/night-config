// Json5 can contain a Header or better said File Comment for version or Author explanation
{
	// This is a commented String value
	"string": "This is a string with a lot of characters to escape \n\r\t \\ \" ",
	"null": null, // Comments can be written after a Value
	"double": 0.123456,
	"float": 0.123456,
	"list": [
		"a",
		"b",
		3,
		null,
		true,
		false,
		17.5
	],
	"config": {
		"boolean": true,
		"false": false
	},
	"dots.in.key": 'value', // Strings also can be single quoted
	"int": 123456,
	"long": 1234567890,
	"enum": "A"
}